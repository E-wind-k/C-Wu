	int arr[5] = { 0 };
	int i, j, min;
	for (i = 0; i < 5; i++) {
		scanf("%d", &arr[i]);
	}
	min = arr[0];
	for (i = 0; i < 4; i++) {
		for (j = 0; j < 4 - i; j++) {
			if (arr[j] < arr[j + 1])
			{
				min = arr[j];
				arr[j] = arr[j + 1];
				arr[j + 1] = min;
			}
		}
	}
